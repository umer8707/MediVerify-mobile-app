{
  "expo": {
    "name": "MediVerify",
    "slug": "authenticate-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "jsEngine": "hermes",
    "splash": {
      "backgroundColor": "#007AFF",
      "resizeMode": "contain"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mediverify.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#007AFF"
      },
      "package": "com.mediverify.app"
    },
    "web": {
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
